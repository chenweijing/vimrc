execute pathogen#infect()
syntax on
filetype plugin indent on

set nocompatible
set backspace=indent,eol,start
set autoindent
set ignorecase
set cindent
set hls is
set hlsearch
set ts=4
set shiftwidth=4
set smarttab
set expandtab
set syntax=c 

" set runtimepath+=~/.vim/bundle/YouCompleteMe 
set nu
set t_Co=256

" airline 
let g:airline_theme = "luna"
let g:airline_powerline_fonts = 1
set history=50

" 关闭提示音
set vb t_vb=
